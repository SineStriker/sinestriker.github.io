function WordsGrow(){var e=["A","B","C","D"],t=new Object;t.ftl=0;t.startGrow=function(e,n,r){t.t=e;t.ft=n;t.fh=r;t.ftl=n.length;t.itv=setInterval(t.grow,20)};t.i=-10;t.grow=function(){if(t.i<t.ftl)if(t.i<0)t.t.html(++t.i%2?"ã€€":"_");else{t.i==0&&t.t.html("");t.t.html(t.ft.substr(0,t.i++)+"_")}else{clearInterval(t.itv);t.t.html(t.fh)}};return t};