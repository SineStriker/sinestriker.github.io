function onPageLoad(){autoLayout();animateIn()}function reset(){$nameTitleDiv.css({marginLeft:0});$jobTitleDiv.css({marginLeft:0});$nameTitle.css({opacity:1});$jobTitle.css({opacity:1});$logo.css({marginTop:"70px",opacity:1});autoLayout()}function animateIn(){$("body").removeClass("init-hid");$("body").css("overflow-x","hidden");var e=$(window).width(),t=(e-930)/2;$nameTitleDiv.css("width",1);var n=function(){$nameTitleDiv.animate({width:"100%"},200,"swing")},r=$nameTitle.text();$nameTitle.text("");var i=function(){WordsGrow().startGrow($nameTitle,r,r)};$jobTitleDiv.css("width",1);var s=function(){$jobTitleDiv.animate({width:"100%"},200,"swing")},o=$jobTitle.text();$jobTitle.text("");var u=function(){WordsGrow().startGrow($jobTitle,o,o)};$logo.hide();var a=function(){$logo.show();var e=$logo.css("marginLeft"),n=$logo.css("marginTop");$logo.css({width:"1px",height:"1px",marginLeft:t+150,marginTop:"170px"});$logo.animate({width:"200px",height:"200px",marginLeft:e,marginTop:n},200,"easeOutBack");$logoImg.css({width:"1px",height:"1px"});$logoImg.animate({width:"200px",height:"200px"},200,"easeOutBack")};$menuA.hide();$menuA.css({marginTop:"45px",opacity:0});var f=function(e){$menuA.eq(e).show();$menuA.eq(e).animate({marginTop:0,opacity:1},200,"easeOutBack")};$bodyWrapper.addClass("init-hid");var l=function(){$("body").css("overflow-x","auto");$bodyWrapper.removeClass("init-hid");$bodyWrapper.hide();$bodyWrapper.fadeIn(400);waitAnimateFunc!=null&&waitAnimateFunc()};n();setTimeout(s,100);setTimeout(a,300);setTimeout(i,400);setTimeout(u,500);setTimeout(function(){f(0)},300);setTimeout(function(){f(1)},320);setTimeout(function(){f(2)},340);setTimeout(function(){f(3)},360);setTimeout(function(){f(4)},380);setTimeout(l,1e3)}function animateOut(e){$("body").css("overflow-x","hidden");isAnimating=!0;$menuA.fadeOut(200);$bodyWrapper.fadeOut(200);var t=function(){$nameTitleDiv.animate({marginLeft:"100%",width:"1px"},200,"swing")},n=function(){$jobTitleDiv.animate({marginLeft:"100%",width:"1px"},200,"swing")},r=$(window).width(),i=(r-930)/2,s=function(){$nameTitle.animate({marginLeft:i,opacity:0},200,"easeInBack")},o=function(){$jobTitle.animate({marginLeft:i,opacity:0},200,"easeInBack")},u=function(){$logo.animate({marginLeft:"80%",opacity:0},200,"easeInBack")};s();setTimeout(o,50);setTimeout(u,200);setTimeout(t,500);setTimeout(n,550);setTimeout(function(){document.location.href=e;isAnimating=!1},800)}function autoLayout(){var e=$(window).width(),t=(e-930)/2;$("#nameTitle").css("marginLeft",t+285);$("#jobTitle").css("marginLeft",t+285);$("#logo").css("marginLeft",t+50);$("#navi-wrapper").css("marginLeft",t+305)}function waitAnimate(e){waitAnimateFunc=e}function trace(e){window.console.info(e)}function chkExternalLink(e){var t=/^http(s?)\:\/\//gi,n=/^http(s?)\:\/\/(www\.{0,1})mousebomb.org/gi;return e.match(t)?e.match(n)?!1:!0:!1}var isAnimating=!1,$nameTitleDiv,$nameTitle,$jobTitleDiv,$jobTitle,$logo,$logoImg,$menuA,$bodyWrapper;$(document).ready(function(){$nameTitleDiv=$("#nameTitleDiv");$nameTitle=$("#nameTitle");$jobTitleDiv=$("#jobTitleDiv");$jobTitle=$("#jobTitle");$logo=$("#logo");$logoImg=$("#logo img");$menuA=$("#main-menu a");$bodyWrapper=$("#body-wrapper");autoLayout();window.onload=onPageLoad;$(window).resize(function(){isAnimating||autoLayout()});$.get("/modules/yanyu.php",{},function(e){$("#yanyu").text("-“"+e+"”")});$("#main-menu a").click(function(){var e=this.href;if(isMobile)return!0;animateOut(e);return!1});window.onunload=function(){}});var waitAnimateFunc=null;